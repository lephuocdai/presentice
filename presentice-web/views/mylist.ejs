<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>New Feed</title>

    <!-- Bootstrap core CSS -->
    <link href="/bootstrap/css/bootstrap.css" rel="stylesheet">

    <!-- Custom CSS for the '3 Col Portfolio' Template -->
    <link href="/css/3-col-portfolio.css" rel="stylesheet">
  </head>

  <body>
    <% include header.ejs %>

    <div class="container">

      <div class="row">

        <div class="col-lg-12">
          <h1 class="page-header">New Feed <small>Videos need your review!</small></h1>
        </div>

      </div>
      <% for(var i=0; i<videoList.length; i++) {%>
        <% if((i%3 == 0)) {%>
            <div class="row">
        <%}%>
        

        <div class="col-md-4 portfolio-item">
          <%- link_to('<img class="img-responsive" src="http://placehold.it/700x400">',videoList[i]['url']) %>
          <h3><%- link_to(videoList[i]['name'], videoList[i]['url']) %></h3>
          <p>Lorem ipsum dolor sit amet, consectetur adipiscing elit. Nam viverra euismod odio, gravida pellentesque urna varius vitae.</p>
        </div>
      <% if((i%3 == 2)) {%>
            </div>
        <%}%>  
      <% } %>
      <hr>

      <div class="row text-center">
        
        <div class="col-lg-12">
          <ul class="pagination">
            <li><a href="#">&laquo;</a></li>
            <li class="active"><a href="#">1</a></li>
            <li><a href="#">2</a></li>
            <li><a href="#">3</a></li>
            <li><a href="#">4</a></li>
            <li><a href="#">5</a></li>
            <li><a href="#">&raquo;</a></li>
          </ul>        
        </div>

      </div>

    </div><!-- /.container -->

    <div class="container">

      <hr>

    <% include footer.ejs %>
      
    </div><!-- /.container -->

    <!-- JavaScript -->
    <script src="/js/parse-1.2.15.min.js"></script>
    <script src="/js/jquery-1.10.2.js"></script>
    <script src="/bootstrap/js/bootstrap.js"></script>
    <script src="/js/home.js"></script>

  </body>

</html>